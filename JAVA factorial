import java.util.Scanner;
public class Ciclo{
    public static void main(String[] arg) {
        long factorial = 1;
        int num;
        Scanner numero = new Scanner(System.in);
        num = numero.nextInt();
        for (int i = num; i >0; i-- ) {
            factorial = factorial *i;
            
        }
        System.out.println(factorial);
    }
}
